var idle = function idle(callback) {
  callbacks.push(callback);

  if (callbacks.length === 1) {
    var next = typeof window !== 'undefined' && window.requestIdleCallback || setTimeout;
    next(function () {
      callbacks.forEach(function (fn) {
        return fn();
      });
      callbacks.length = 0;
    });
  }
