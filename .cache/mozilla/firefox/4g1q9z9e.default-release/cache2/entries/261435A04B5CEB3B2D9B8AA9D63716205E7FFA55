function all(fns, callback) {
  var result = [];
  var complete = 0;

  var done = function done(index) {
    return function (value) {
      result[index] = value;
      complete++;

      if (complete === fns.length) {
        callback(result);
      }
    };
  };

  fns.forEach(function (fn, index) {
    return fn(done(index));
  });
}

function getClassesForLocale(li, options) {
