  }, {
    key: "callback",
    value: function callback(onSuccess, onFailure, onComplete, msg) {
      if (this.cancelled) {
        return;
      } // remove timeout job


      this.timeoutJob.stop();
      var parsedMsg;

      if (msg == null) {
        parsedMsg = {
          errorCode: -1,
          errorText: "Invalid response: ".concat(msg),
          returnValue: false
        };
      } else {
        try {
          parsedMsg = JSON.parse(msg);
        } catch (e) {
          parsedMsg = {
            errorCode: -1,
            errorText: msg,
            returnValue: false
          };
        }
      }

      if (parsedMsg.errorCode || parsedMsg.returnValue === false) {
        if (onFailure) {
          onFailure(parsedMsg);
        }
      } else if (onSuccess) {
        onSuccess(parsedMsg);
      }

      if (onComplete) {
        onComplete(parsedMsg);
      }

      if (!this.subscribe) {
        this.cancel();
      }
    }
    /**
     * Cancel the current LS2 request.
     *
     * @method
     * @memberof webos/LS2Request.LS2Request.prototype
     * @returns {undefined}
     * @public
     */

  }, {
    key: "cancel",
    value: function cancel() {
      // remove timeout job
      this.timeoutJob.stop();
      this.cancelled = true;

      if (this.bridge) {
        this.bridge.cancel();
        this.bridge = null;
      }

      if (this.ts && refs[this.ts]) {
        delete refs[this.ts];
      }
    }
  }]);

  return LS2Request;
}();

exports["default"] = LS2Request;

